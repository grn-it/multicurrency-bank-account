{
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.14",
        "phpmd/phpmd": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.23",
        "slevomat/coding-standard": "^8.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\UseCases\\": "useCases/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
